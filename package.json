    "sync": "browser-sync start --files='./views','./public' --https --proxy=localhost:8080",
    "build": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "markdown-it": "^10.0.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "jquery": "^3.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "popper.js": "^1.16.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
